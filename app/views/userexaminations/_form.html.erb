<div class="form-container">
   <%= form_for @userexamination,:url=>{:action => 'create'} do %>
<%=  error_messages_for 'userexamination' %>
   <div>
       <label>
           User Name:
       </label>
       <%= @user_name %>
       <%= hidden_field_tag  'user_id', @user_id %>
   </div>
   <hr/>
   <div>
       <label>
           Present Assigned Examinations:
       </label>
       <%if @userexam.count == 0 %>
       NO Examination is Assigned Yet.<%else %>
       <% @userexam.each do |a| %>
       <%= a.examination.examname %> ,
       <%end %>
       <%end %>
   </div>
   <hr>
   <div>
       <label>
           Avialable Examinations:
       </label>
        <% @examinations.each do |exam| %>  
            <% unless Userexamination.exists?(:user_id=>@user_id,:examination_id=>exam.id) %>
              <%=check_box_tag "exam[exam_ids][]", exam.id,false %><%=exam.examname %>
            <% end %>
        <% end %>
   </div>
   <hr>
   <div class="buttonrow">
       <%= submit_tag "Create" %>
   </div>
   <% end %>
</div>