<div class="form-container">
    <% form_for([@user],:html=>{:class=>'cmxf'}) do |f| %>
    <p class="legend">
        <strong>Note:</strong>
        Required fields are marked with an asterisk (<em>*</em>)
    </p>
    <fieldset>
        
        <div>
            <label for="name">
                First Name<em>*</em>
            </label>
            <%= f.text_field :first_name %>
        </div>
		        <div>
            <label for="name">
                Last Name<em>*</em>
            </label>
            <%= f.text_field :last_name %>
        </div>
        <div>
            <label for="email">
                Email <em>*</em>
            </label>
            <%= f.text_field :email %>
        </div>

            <%= f.hidden_field :password,:value=>"123456" %>

            <%= f.hidden_field :password_confirmation,:value=>"123456" %>
 
 
        <div>
            <label for="phone_number">
                Primary Contact Number<em>*</em>
            </label>
            <%= f.text_field :mobile_number %>
        </div>
        <div>
            <label for="phone_number">
                Secondary Contact Number
            </label>
            <%= f.text_field :phone_number %>
        </div>

        <div class="buttonrow">
            <%= f.submit button_name ,:class=>'button' %>
        </div>
    </fieldset>
    <% end %>
</div>
