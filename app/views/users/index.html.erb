<h1>List of  Users</h1>
<div class="flickr_pagination">
<%= alphabar @paginator %>
</div>
<table id="box-table-a">
    <tr>
       <th scope="col">
            Enrollment Number
        </th>
        <th scope="col">
            First Name
        </th>
		        <th scope="col">
            Last Name
        </th>
        <th scope="col">
            Email
        </th>
        <th scope="col">
            Actions
        </th>
    </tr>
    <% @users.each do |user| %>
	<%if user.id == 1 || user.id == 2%>
    <tr>
       <td>
            ----
        </td>
        <td>
            <%= user.first_name %>
        </td>
		<td>
            <%= user.last_name %>
        </td>
        <td>
            <%= user.email %>
        </td>
        <td>
            <%= link_to 'View', user %> | <%= link_to 'Edit', edit_user_path(user) %> 
        </td>
    </tr>
	<%else%>
    <tr>
        <td>
            <%= user.enrollment_number %>
        </td>
        <td>
            <%= user.first_name %>
        </td>
		 <td>
            <%= user.last_name %>
        </td>

        <td>
            <%= user.email %>
        </td>
        <td>
            <%= link_to 'View', user %> | <%= link_to 'Edit', edit_user_path(user) %> | <%= link_to 'Delete', user, :confirm => 'Are you sure?', :method => :delete %> | <%= link_to 'Assign Examination', :controller => "userexaminations", :action=>"new", :user_id=> user.id %> | <%= link_to 'Examination Status', :controller => "userexaminations", :action=>"exam_status", :user_id=> user.id %>
        </td>
        </td>
    </tr>
	
	<%end%>
    <% end %>
</table>

<br/>
<%=link_to new_user_path,:class=>'more' do %>Create New User<% end %>
&nbsp;&nbsp;&nbsp;
<%= link_to 'Import Users',{:action=>'upload'},:class=>'more' %>
&nbsp;&nbsp;&nbsp;
<%= link_to 'Export Users',{:action=>'export'},:class=>'more' %>
